#!/bin/sh

IPSET=`which ipset`

echo "Lista de portas desviadas para o proxy"
$IPSET -exist create proxy bitmap:port range 1-65535
$IPSET flush proxy
for i in $(cat /etc/sgipset/proxy); do $IPSET add proxy $i; done

echo "Lista de portas SSL desviadas para o proxy"
$IPSET -exist create proxy-ssl bitmap:port range 1-65535
$IPSET flush proxy-ssl
for i in $(cat /etc/sgipset/proxy-ssl); do $IPSET add proxy-ssl $i; done
